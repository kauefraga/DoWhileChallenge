datasource db {
  provider = "sqlite"
  url      = "./database.db"
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id      Int    @id @default(autoincrement())
  subject String @unique
  body    String @unique
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
